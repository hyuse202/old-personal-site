---
import PageTitle from "~/components/page-title/index.astro";
import PageSubtitle from "~/components/page-subtitle/index.astro";
import Layout from "~/layouts/Layout.astro";
import data from "~/features/projects-page/index";
import ProjectList from "~/features/projects-page/components/project-list.astro";
---

<Layout title="Projects">
  <div class="flex flex-col">
    <PageTitle title="Projects" />
    <PageSubtitle
      content="List of projects I recently worked on and working on."
    />
    <ProjectList data={data} />
  </div>
</Layout>
